{
    "network": {
        "number_of_vms": 3,
        "number_of_changes": 1,
        "prefixe": "192.168.10.",
        "services": [{
                "name": "ssh",
                "commands": [{
                        "name": "ssh",
                        "parameters": [
                            "tester@&ip"
                        ]
                    },
                    {
                        "name": "sftp",
                        "parameters": [
                            "tester@&ip"
                        ]
                    }
                ]
            },
            {
                "name": "proftpd",
                "commands": [{
                        "name": "wget",
                        "parameters": [
                            "ftp://tester:test@&ip/hello.txt"
                        ]
                    },
                    {
                        "name": "curl",
                        "parameters": [
                            "ftp://tester:test@&ip/hello.txt"
                        ]
                    }
                ]
            },
            {
                "name": "apache2",
                "commands": [{
                        "name": "wget",
                        "parameters": [
                            "&ip",
                            "-r &ip"
                        ]
                    },
                    {
                        "name": "curl",
                        "parameters": [
                            "&ip"
                        ]
                    }
                ]
            }
        ]
    },
    "experiment": {
        "start_date": "2019-05-02 15:00",
        "end_date": "2019-05-03 15:00",
        "max_actions_per_vm": 9600
    },
    "hacker": {
        "attacks": [{
            "name": "nmap",
            "parameters": [
                "-T0 -sV --top-ports 5 &ip"
            ]
        }]
    }
}
